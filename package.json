{
  "name": "@kmariappan/strapi-client-js",
  "author": "Karthikeyan Mariappan",
  "description": "Javascript client for Strapi Rest API",
  "version": "0.0.0-development",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "postinstall": "tsc",
    "dev": "nodemon --config nodemon.json src/example/play.ts",
    "lint": "eslint  . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "semantic-release": "semantic-release --branches main",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint"
    }
  },
  "dependencies": {
    "axios": "^1.6.1",
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "@types/qs": "^6.9.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "ansi-regex": "^6.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.3.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "semantic-release": "^22.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kmariappan/strapi-client-js.git"
  }
}
